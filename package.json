{
  "name": "ih-lib60870-node",
  "version": "1.0.10",
  "description": "Cross-platform Node.js native addon of the iec60870 support",
  "main": "index.js",
  "scripts": {
    "configure": "node-gyp configure",
    "build": "node-gyp build",
    "prebuild": "prebuild --target 10.0.0 --target 12.0.0 --target 14.0.0 --target 16.0.0 --target 18.0.0 --target 20.0.0",    
    "prebuild-upload": "prebuild --upload-all",
    "install": "node install-all.js || npm run build"
  },
  "keywords": [
    "native",
    "addon",
    "cross-platform"
  ],
  "author": "Amigo9090",
  "license": "MIT",
  "files": [
    "builds/",
    "index.js",
    "binding.gyp",
    "install-all.js",
    "lib/"
  ],
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^7.1.1",
    "prebuild-install": "^7.1.1",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "mocha": "10.2.0",
    "prebuild": "12.1.0",
    "node-gyp": "10.x"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64",
    "arm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/intrahouseio/ih-lib60870-node.git"
  },
  "homepage": "https://github.com/intrahouseio/ih-lib60870-node#readme"
